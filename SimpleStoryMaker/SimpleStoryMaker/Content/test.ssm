PLAYER{
    name:"";
    hp:100;
    ad:true == false;
}

var asd = !(true or true);
var c = 100;
var b = -(2 * c / 5);

SCENE START{
    write:"";
    choices{
        r{
            text:"Go right " + b;
            goto: START;
        }
        left{
            text:"Go left " + asd;
            goto: left;
        }
    }
}

SCENE left{
    write:"the left";
    choices{
        f{
            text: "go further";
            goto:further;
        }
    }
}

SCENE further{
    write:"go to end or the start";
    choices{
        x{
            text:"go end";
            goto:almostEnd;
        }
        start{
            text:"go start";
            goto:START;
        }
    }
}

SCENE almostEnd{
    write:"Almost there";
    goto: END;
}
SCENE END{
    write:"you've reached the end";
}